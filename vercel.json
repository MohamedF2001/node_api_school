{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api-docs",
      "dest": "/public/swagger.html" 
    },
    {
      "src": "/swagger.json",
      "dest": "/public/swagger.json"
    }
  ]
}
